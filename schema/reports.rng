   <define name="link">
      <element name="link">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Added automatically by the processing tools. Do not add or edit.</a:documentation>
         <empty/>
         <optional>
            <attribute name="href">
